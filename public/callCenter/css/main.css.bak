body,
div,
ul,
ol,
li,
p,
i,
em,
h1,
h2,
h3,
h4,
a,
strong,
span,
img,
form,
input,
textarea,
button {
    padding: 0;
    margin: 0
}

body {
    font-family: microsoft yahei, simsun, Tahoma, Arial, helvetica neue, hiragino sans gb, sans-self;
    font-size: 14px;
    line-height: 24px
}

@font-face {
    font-family: iconfont;
    src: url(font/iconfont.eot);
    src: url(font/iconfont.eot?#iefix) format('embedded-opentype'), url(font/iconfont.woff) format('woff'), url(font/iconfont.ttf) format('truetype'), url(font/iconfont.svg#iconfont) format('svg')
}

input,
textarea {
    outline: 0
}

li {
    list-style: none
}
a {
    text-decoration: none;
    color: inherit
}

img {
    border: 0
}

.pr {
    position: relative
}

.pa {
    position: absolute
}

.pf {
    position: fixed
}

.f-l {
    float: left
}

.f-r {
    float: right
}

li,
div,
ul {
    *zoom: 1
}
.clear-both{
    clear: both;
}
.container{
    width: 100%;
}
.customer-chat-top{
    background: #36a9e1;
    color: #ffffff;
    height: 45px;
    line-height: 45px;
    position: relative;
    width: 100%;
    color: #fff;
    margin: 0;
    padding: 0;
    cursor: pointer;

}

.chat-window-top{
    position: fixed;
    top: 0;
    left: 0;
    z-index: 999999;
}

.customer-chat-header-title{
    float: left;
    padding-left: 4px;
    font-size: 15px;
    margin-left: 12px;
    color: #ffffff;
}

#customer-chat-button-settings{
    float: right;
    width: 20px;
    height: 20px;
    margin-right: 20px;
    background: url(../img/glyphicons-halflings-white.png);
    background-position: -354px -137px;
    background-repeat: no-repeat;
    margin-top: 10px;
}
.customer-chat-header-menu {
    display: none;
    border-left: 2px solid #36a9e1;
    border-right: 2px solid #36a9e1;
    border-bottom: 3px solid #36a9e1;
    position: absolute;
    width: 100%;
    height: auto;
    top: 100%;
    right: 0;
    z-index: 9999;
    background: #fff;
    padding: 16px;
    border-bottom-left-radius: 10px;
    border-bottom-right-radius: 10px;
    box-sizing: border-box;
    margin: 0;
    color: #4d4d4d;
}
.customer-chat-header-menu-triangle {
    position: absolute;
    width: 0;
    height: 0;
    top: -20px;
    right: 16px;
    border: 10px solid transparent;
    border-bottom-color: #fff;
    pointer-events: none;
}

.customer-chat-header-menu > a {
    display: block;
    position: static;
    clear: both;
    line-height: 1.5em;
    text-decoration: none;
    color: #000;
    clear: both;
    padding: 8px;
}
.icon-music {

    display: inline-block;
    width: 14px;
    height: 14px;

    line-height: 14px;
    vertical-align: text-top;
    background-image: url(../img/glyphicons-halflings.png);
    background-position: 14px 14px;
    background-repeat: no-repeat;
    margin-top: 4px;
    background-position: -24px 0;
}
.icon-off {
    display: inline-block;
    width: 14px;
    height: 14px;
    line-height: 14px;
    vertical-align: text-top;
    background-image: url(../img/glyphicons-halflings.png);
    background-position: 14px 14px;
    background-repeat: no-repeat;
    margin-top: 4px;
    background-position: -384px 0;


}
.customer-chat-header-menu a div{
    display: inline-block;
    margin-left: 20px;
}
.customer-chat-disabled div {
    text-decoration: line-through;

}
.customer-chat-disabled{
    opacity: 0.35;
}
.customer-chat-select-avatar{
    height: 60px;
    text-align: center;
    margin-top: 40px;
    line-height: 60px;
}

.back{
    display: block;
    float: left;
    width: 20px;
    height: 20px;
    position: absolute;
    top: 13px;
    left: 10px;
}
.cardNumber{
    width: 100%;
    text-align: center;
    padding-left: 0;
}
.icon-white{
    padding: 5px 5px;
    height: 14px;
    border-radius: 3px;
    background-color: #86C953;
    vertical-align: top;
    display: inline-block;
    text-align: center;
    cursor: pointer;
    margin-top: 9px;
}
.icon-chevron-left{
    display: block;
    width: 14px;
    height: 14px;
    background-image: url(../img/glyphicons-halflings-white.png);
    background-position:-432px -72px;
    vertical-align: middle;

}

.icon-chevron-right{
    display: block;
    width: 14px;
    height: 14px;
    background-image: url(../img/glyphicons-halflings-white.png);
    background-position: -456px -72px;

}
.icon-left{
    margin-right: 10px;
}
.icon-right{
    margin-left: 10px;
}
.form-content{
    width: 320px;
    margin: 0 auto;
}
.form-group{
    width: 310px;
    margin: 0 auto;
}
.form-group input{
    display: block;
    margin: 0 auto;
    width: 286px;
    height: 40px;
    border: 2px solid #e5e5e5;
    font-weight: bold;
    font-size: 12px;
    color: #999;
    padding:0 10px;
    border-radius:3px;
}

.form-group input.account{
    margin-top: 20px;
}
.form-group input:focus{
    box-shadow: none;
    border-color: #94d662;
    color: #76b447;
}
.has-error input{
    border-color: #f77;
}
.help-block{
    color: #f77;
}
.yd-div{
    margin-top: 8px;
    clear: both;
}
.yd-div a{
    font-size: 12px;
    color: #76b447;
}
a.forget{
    float: left;
    margin-left: 12px;
}
a.newUser{
    float: right;
    margin-right: 12px;
}
#nextBtn{
    display: block;
    padding: 8px 22px;
    background: #86C953;
    color: #fff;
    border: none;
    border-bottom: 2px solid #76B447;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
    margin: 0 auto;
    margin-top: 40px;
    outline: none;
    cursor: pointer;
}
.chat-content{
    width: 100%;
    margin: 0 auto;
    padding-bottom: 50px;
    margin-top: 45px;
    background-color:#cccccc17;
}
.customer-chat-content-message-operator{
    padding: 16px;
    clear: both;
    overflow: hidden;
}
.customer-chat-content-message-avatar-operator{
    float: left;
    width: 40px;
    height: 40px;
    border-radius: 50%;
    /*background:url(../img/b.png) no-repeat;*/
}

.customer-chat-content-message-avatar-operator img {
    width: 40px;
    height: 40px;
}
.customer-chat-content-message-column {
    width: 100%;
    float: none;
    padding-left: 60px;
    padding-right: 20px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}

.customer-chat-content-message-author{
    font-size: 14px;
    float: left;
    color: #0879b1;
    font-weight: 600;
}
.customer-chat-content-message-time{
    float: right;
    color: #ccc;
    font-size: 12px;
}
.customer-chat-content-message-body {
    font-size: 12px;
    line-height: 1.75em;
    word-break: break-word;
    word-wrap: break-word;
    text-align: left;
    clear: both;
}
.customer-chat-content-message-avatar-operator{
    position: absolute;
}

.footer{
    position: fixed;
    left: 6px;
    right: 6px;
    padding: 2px;
    height: 50px;
    border: none;
    background: none;
    bottom: 0;

}
.customer-chat-content-message-input-field{
    color: #999;
    border: 2px solid #e5e5e5;
    font-weight: bold;
    font-size: 12px;
    background: #fff;
    margin: 0;
    width: 100%;
    position: absolute;
    left: 0;
    right: 0;
    padding: 16px 90px 16px 16px;
    box-sizing:border-box;

}
.customer-chat-content-message-input-field:focus{
    border-color: #94d662;
    color: #76b447;
}
.file-button {
    overflow: hidden;
    position: absolute;
    width: 18px;
    height: 18px;
    font-size: 18px;
    color: #36a9e1;
    cursor: pointer;
    right: 86px;
    top: 18px;
}
#file-input{
    display: none;
}
.fa-upload{
    display: block;
    width: 16px;
    height: 14px;
    background: url(../img/up.png) no-repeat center center;
    background-size: cover;
    margin-top: 4px;
}
.customer-chat-content-message-emots-button {
    right: 16px;
    top: 11px;
}
.customer-chat-content-message-emots-button {
    position: absolute;
    right: 56px;
    top: 13px;
    line-height: 30px;
    text-align: center;
    font-weight: 600;
    color: #ccc;
    width: 30px;
    height: 30px;
    cursor: pointer;
}

.btn-send {
    position: absolute;
    right: 10px;
    top: 13px;
    line-height: 30px;
    text-align: center;
    font-weight: 600;
    color: #ccc;
    height: 30px;
    cursor: pointer;
}

.customer-chat-content-message-emots-button:after {
    content: '';
    display: block;
    background: url(../img/emots.png) no-repeat -18px 0;
    width: 18px;
    height: 18px;
    position: absolute;
    top: 50%;
    left: 50%;
    margin: -9px 0 0 -9px;
}

.emots-wrapper{
    margin-top: 5px;
}

.customer-chat-emots-menu {
    border: 2px solid #e0e0e0;
    /* padding: 10px;*/
    width: 315px;
    padding: 0px 2px;
    bottom: 60px;
    right: 2px;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 5px;
}
.customer-chat-emots-menu {
    top: auto;
    height: 64px;
    line-height: 18px;

}
.customer-chat-emots-menu {
    display: none;
    position: absolute;
    z-index: 9999;
    background: #fff;

}
.customer-chat-emots-menu .customer-chat-header-menu-triangle {
    border: 7px solid transparent;
    border-top-color: #e0e0e0;
    right: 20px;
    top: 100%;
    position: absolute;
    width: 0;
    height: 0;

    pointer-events: none;
}

.customer-chat-emoticon {
    margin-left: 8px;
    margin-bottom: 10px;
    width: 18px;
    height: 18px;
    display: inline-block;
    text-decoration: none;
    overflow: hidden;
}


