.id-card{
    margin-top: 20px !important;
}
.id-sure-btn{
    display: block;
    margin: 0 auto;
    width: 310px;
    height: 40px;
    border: none;
    font-weight: bold;
    font-size: 12px;
    color: #999;
    margin-top: 20px;
    border-radius: 3px;
    outline: none;
}

.addclass{
    background-color:#86C953;
    color: #fff;
}

.mychat .myChatHead{
    position: absolute;
    float: right;
    width: 40px;
    height: 40px;
    border-radius: 50%;
    /*background: url(../img/a.png) no-repeat;*/
    top: 16px;
    right: 16px;
}
.mychat .myChatCon{
    width: 100%;
    float: none;
    padding-right: 60px;
    padding-left: 20px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
.mychatoperator{
    width: 100%;
    padding: 16px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    position: relative;
}
.mychatTime{
    float: left;
    color: #ccc;
    font-size: 12px;
}
.mychatAuthor{
    font-size: 14px;
    float: right;
    color: #0879b1;
    font-weight: 600;
}
.mychatMess{
    font-size: 12px;
    line-height: 1.75em;
    word-break: break-word;
    word-wrap: break-word;
    text-align: right;
    clear: both;
}
.mychatoperator .mychatMessDe{
    font-size: 12px;
    line-height: 1.75em;
    word-break: break-word;
    word-wrap: break-word;
    text-align: left;
    float: right;
    clear: both;
}

.mychatoperator .mychatMessDe div{
    text-align: left;
    float: right;
    clear: both;
    width: fit-content;

}

.serviceChat .mychatMessDe{
    font-size: 12px;
    line-height: 1.75em;
    word-break: break-word;
    word-wrap: break-word;
    text-align: left;
    clear: both;
}
.mychatMessDe .upPic,.mychatMessDe img{
    max-width: 150px;
    border-radius: 5px;
    background-color: #fff;
}
.record{
    position: fixed;
    left: 6px;
    right: 6px;
    padding: 2px;
    border: none;
    background-color: #fff;
    z-index: 999;
    bottom: 50px;
    border-radius: 5px;
    display: none;
}

.record ul{
    width: 100%;
    /*background-color: #ccc;*/
    border: 1px solid #ccc;
    box-sizing: border-box;
    border-radius: 5px;
    max-height: 156px;
    overflow-y: scroll;
    /*box-shadow: ;*/
}

.record ul li{
    /*height: 30px;*/
    font-size: 14px;
    /*line-height: 30px;*/
    text-align: left;
    padding:2px 0 2px 10px;
    border-bottom: 1px solid #eee;
}
.record ul li:hover{
    background-color: #ccc;
}

/* forget页面 */
.error-message{
    color: red;
    font-size: 12px;
}